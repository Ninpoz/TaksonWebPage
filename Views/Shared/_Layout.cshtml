<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - Takson AB</title>

    <!-- Tailwind CSS -->
    <link rel="stylesheet" href="~/css/site.min.css" />


    <!-- Flowbite -->
    <script src="~/node_modules/flowbite/dist/flowbite.min.js" defer></script>
    <!-- Font Awesome CDN -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" />
</head>
<body class="bg-gray-700 font-sans antialiased">

   

    <!-- Main Content -->
    <main class="container mx-auto">
        <!-- Navbar (Flowbite Component) -->
        <nav class="sticky top-0 z-50 rounded-t-lg border-gray-200 bg-white dark:bg-gray-900">
            <div class="mx-auto flex max-w-screen-xl flex-wrap items-center justify-between p-4">
                <a href="/" class="flex items-center">
                    <img src="/images/file.png" class="h-12" alt="Logo" />
                    <span class="self-center whitespace-nowrap text-2xl font-semibold dark:text-white">Takson AB</span>
                </a>
                <button data-collapse-toggle="navbar-default" type="button" class="inline-flex h-10 w-10 items-center justify-center rounded-lg p-2 text-sm text-gray-500 hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 md:hidden dark:hover:bg-gray-700 dark:focus:ring-gray-600" aria-controls="navbar-default" aria-expanded="false">
                    <span class="sr-only">Open main menu</span>
                    <svg class="h-5 w-5" aria-hidden="true" fill="none" viewBox="0 0 17 14" xmlns="http://www.w3.org/2000/svg">
                        <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M1 1h15M1 7h15M1 13h15"></path>
                    </svg>
                </button>
                <div class="hidden w-full md:block md:w-auto" id="navbar-default">
                    <ul class="mt-4 flex flex-col rounded-lg border border-gray-100 bg-gray-50 p-4 font-medium dark:bg-gray-800 dark:border-gray-700 md:p-0 md:flex-row md:space-x-8 md:mt-0 md:border-0 md:bg-white md:dark:bg-gray-900">
                        <li><a href="/" class="block rounded bg-blue-700 px-4 py-2 text-white dark:text-white md:bg-transparent md:text-blue-700 md:p-0">Home</a></li>
                        <li><a href="/OmOss" class="block px-4 py-2 text-gray-700 hover:bg-gray-100 dark:text-white">Om Oss</a></li>
                        <li><a href="/VåraTjänster" class="block px-4 py-2 text-gray-700 hover:bg-gray-100 dark:text-white">Våra Tjänster</a></li>
                        <li><a href="/contact" class="block px-4 py-2 text-gray-700 hover:bg-gray-100 dark:text-white">Contact</a></li>
                    </ul>
                </div>
            </div>
        </nav>
       
        @RenderBody()
       
    </main>

    <h1 class="text-center text-white"> Följ oss gärna på våra sociala medier</h1>
    <!-- Social Media Icons -->
    <div class="flex justify-center space-x-12 py-4">
        <a href="https://www.facebook.com/taksonab/?locale=sv_SE" target="_blank" rel="noopener noreferrer" class="text-6xl text-gray-400 hover:text-white"><i class="fab fa-facebook"></i></a>
        <a href="https://www.instagram.com/takson_ab/" target="_blank" rel="noopener noreferrer" class="text-6xl text-gray-400 hover:text-white"><i class="fab fa-instagram"></i></a>
    </div>



    <!-- Footer -->
    <footer class="bg-gray-700 py-6 text-center text-white">
        <p>&copy; Powered by A.Bergman IT-Solutions.</p>
      
    </footer>

</body>
</html>
